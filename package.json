{
  "name": "edgedriver",
  "version": "4.17134.1",
  "description": "Microsofts' EdgeDriver for Node.js",
  "homepage": "https://webdriver.io",
  "license": "Apache-2.0",
  "author": {
    "name": "Barrett Sonntag",
    "email": "barrett@sosuke.com",
    "url": "https://www.sosuke.com"
  },
  "keywords": [
    "edgedriver",
    "edgedriverserver",
    "webdriver"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/barretts/node-edgedriver.git"
  },
  "main": "./dist/cjs/edgedriver",
  "exports": "./dist/index.js",
  "type": "module",
  "bin": {
    "edgedriver": "./bin/edgedriver"
  },
  "scripts": {
    "build": "tsc -b .",
    "clean": "rimraf ./node_modules package-lock.yaml ./dist",
    "prepare": "husky install",
    "install": "test -f ./dist/install.js && node ./dist/install.js || echo \"Skipping install, project not build!\"",
    "test": "run-s test:*",
    "test:lint": "eslint",
    "test:e2e": "node  --loader ts-node/esm ./tests/e2e.test.ts",
    "watch": "npm run build -- --watch"
  },
  "devDependencies": {
    "@types/node": "^20.1.5",
    "@types/unzipper": "^0.10.6",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "@vitest/coverage-c8": "^0.31.0",
    "eslint": "^8.40.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-unicorn": "^47.0.0",
    "husky": "^8.0.3",
    "npm-run-all": "^4.1.5",
    "release-it": "^15.10.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "wait-port": "^1.0.4",
    "webdriverio": "^8.10.2"
  },
  "dependencies": {
    "@wdio/logger": "^8.6.6",
    "camaro": "^6.2.0",
    "decamelize": "^6.0.0",
    "edge-paths": "^3.0.5",
    "unzipper": "^0.10.14",
    "which": "^3.0.1",
    "yargs": "^17.7.2"
  }
}
